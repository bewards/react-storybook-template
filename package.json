{
  "name": "rp-react-best-practices",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.1",
    "bootstrap": "^5.1.3",
    "classnames": "^2.3.1",
    "cuid": "^2.1.8",
    "faker": "5.5.3",
    "formik": "^2.2.9",
    "lodash": "^4.17.21",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-hook-form": "^7.29.0",
    "react-intl": "^6.0.4",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "reselect": "^4.1.5",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "analyze": "npm run build && source-map-explorer 'dist/assets/*.js' --no-border-checks",
    "test": "vitest",
    "test:cov": "vitest run --coverage && open './coverage/index.html'",
    "test:ci": "vitest run",
    "lint": "eslint \"./src/**/*.{js,jsx,ts,tsx}\" --fix",
    "folderslint": "folderslint",
    "prettier": "prettier --write './src/**/*.{js,jsx,ts,tsx,css,scss}'",
    "stylelint": "stylelint --fix './src/**/*.{scss,css}'",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook -s public",
    "lighthouse": "lighthouse http://localhost:3000 --output html --output-path ./.lighthouse/report.html --view",
    "lighthouse:dev": "echo 'add dev url when available'",
    "lhci": "lhci --config=lighthouserc.js autorun",
    "lhci:nightly:desktop": "lhci --config=.github/workflows/nightly-lighthouse-desktop.js autorun",
    "lhci:nightly:mobile": "lhci --config=.github/workflows/nightly-lighthouse-mobile.js autorun",
    "type-check": "tsc --noEmit",
    "husky:pre-commit": "tsc --noEmit && concurrently lint-staged folderslint",
    "husky:pre-push": "vitest run",
    "prepare": "husky install",
    "preview": "vite preview"
  },
  "lint-staged": {
    "./src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "./src/**/*.{scss,css}": [
      "prettier --write",
      "stylelint --fix"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@axe-core/react": "^4.4.2",
    "@formatjs/cli": "^4.8.4",
    "@lhci/cli": "^0.9.0",
    "@storybook/addon-a11y": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/addons": "^6.5.9",
    "@storybook/builder-vite": "^0.1.38",
    "@storybook/node-logger": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/classnames": "^2.3.1",
    "@types/node": "^17.0.32",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "5.3.3",
    "@types/redux-logger": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@vitejs/plugin-react": "^1.3.2",
    "babel-loader": "^8.2.5",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "c8": "^7.12.0",
    "camelcase": "^7.0.0",
    "concurrently": "^7.1.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-check-file": "^1.1.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "4.5.0",
    "eslint-plugin-storybook": "^0.5.13",
    "eslint-plugin-testing-library": "^5.5.1",
    "fetch-mock": "^9.11.0",
    "folderslint": "^1.0.0",
    "global-jsdom": "^8.5.0",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^20.0.0",
    "lighthouse": "^9.5.0",
    "lint-staged": "^12.4.1",
    "node-fetch": "^3.2.6",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "redux-devtools-extension": "^2.13.9",
    "redux-logger": "^3.0.6",
    "routington": "^1.0.3",
    "sass": "^1.52.1",
    "source-map-explorer": "^2.5.2",
    "stylelint": "^14.8.2",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-high-performance-animation": "^1.6.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.7.4",
    "vite": "^2.9.12",
    "vite-plugin-svgr": "^2.1.1",
    "vitest": "^0.17.0",
    "whatwg-fetch": "^3.6.2"
  }
}
